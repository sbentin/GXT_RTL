<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='widgetdashboard'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <!-- Inherit the Ext js Framework stuff.                        -->
  <inherits name='com.extjs.gxt.ui.GXT'/>
  
  <inherits name='com.google.gwt.sample.showcase.Showcase' />
  <inherits name='com.extjs.gxt.samples.Examples' />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.StandardRTL'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.DarkRTL'/> -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.centimia.gxt.sample.widgetdashboard.client.WidgetDashboard'/>
  

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <extend-property name='locale' values='iw'/>
  
</module>
